<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--公司 -->
<mapper namespace="com.lzy.parttime.dao.CompanyDao">
	<!-- 公司列表 -->
	<select id="getCompanyList" resultType="company">
	SELECT
		c.id,
		c. NAME,
		c.addTime,
		c.introduce
	FROM
		`company` c
	WHERE
		c.delFlag = 0;
	</select>
</mapper>